{
  "name": "vue3-ele-components",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "genemock": "npx tsc ./data.ts && node ./data.js",
    "format": "prettier --write \"src/**/*.{js,vue,scss,css}\"",
    "build:comps": "node ./script/buildProject",
    "lib": "npm run build:comps",
    "commit": "git-cz",
    "lint:commit": "commitlint --from=HEAD~1 --to=HEAD"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    },
    "lint-staged": {
      "*.{js,ts,vue}": [
        "eslint --fix",
        "prettier --write",
        "git add"
      ]
    },
    "husky": {
      "hooks": {
        "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
        "pre-commit": "lint-staged"
      }
    }
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@types/lodash": "^4.17.7",
    "@wangeditor/editor-for-vue": "^5.1.12",
    "axios": "^1.7.4",
    "element-plus": "^2.7.8",
    "json-templater": "^1.2.0",
    "lodash": "^4.17.21",
    "mi-encapsulate-components": "^1.0.2",
    "sass": "^1.77.8",
    "sass-loader": "^16.0.0",
    "uppercamelcase": "^3.0.0",
    "vue": "^3.4.31",
    "vue-router": "^4.4.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@element-plus/icons": "^0.0.11",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.27.0",
    "file-save": "^0.2.0",
    "fs-extra": "^11.2.0",
    "husky": "^9.1.5",
    "lint-staged": "^15.2.9",
    "mockjs": "^1.1.0",
    "prettier": "^3.3.3",
    "vite": "^5.3.4"
  }
}
